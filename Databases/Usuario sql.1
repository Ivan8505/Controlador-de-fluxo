CREATE DATABASE ControleDeFluxo;

USE ControleDeFluxo;

CREATE TABLE Usuario (
	IDUser INT NOT NULL AUTO_INCREMENT,
    UsernameUser VARCHAR(15) UNIQUE NOT NULL,
    PasswordUser VARCHAR(50) NOT NULL,
	StatusUser BOOLEAN NOT NULL,
	NameUser VARCHAR(25) NOT NULL,
    OldUser VARCHAR(2) NULL,
    AdressUser VARCHAR(150) NULL,
    
    PRIMARY KEY (IDUser)
);

